{
  "compilerOptions": {
    "noPropertyAccessFromIndexSignature": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "module": "preserve",
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@temp-nx/shared-types": ["../../libs/shared-types/src/index.ts"],
      "@temp-nx/data-models": ["../../libs/data-models/src/index.ts"]
